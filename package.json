    "local:setup": "pnpm supabase:start",
    "local:stop": "pnpm supabase:stop",
    "start": "next start",
    "supabase:start": "supabase start",
    "supabase:status": "supabase status",
    "supabase:stop": "supabase stop",
    "supabase:test": "supabase test db",
    "supabase:typegen": "supabase gen types typescript --local > types/supabase.ts",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
